/*******************************
         Site Overrides
*******************************/
.ui.header {
  display: inline-block;
}

#indexHeader {
    color: @blockTextColor;
    width: @blockWidth;
    font-weight: @bold;
    font-size: @blockFontSize;
    letter-spacing: @letterSpacing;
    height: @blockHeight;
}

#enterHeader {
  font-weight: 700;
  font-family: @pageFont;
  color: rgb(56, 56, 56);
  margin-left: 20px;
}
